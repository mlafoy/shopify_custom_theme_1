<div class="page-width" data-section-id="{{ section.id }}" data-section-type="quotes">
  {% if section.settings.title != blank %}
    <div class="section-header text-center">
      <h2>{{ section.settings.title | escape }}</h2>
    </div>
  {% endif %}

  {% if section.blocks.size > 0 %}
    <div class="quotes-wrapper cst">
      <div class="quotes-slider" id="Quotes-{{ section.id }}" data-count="{{ section.blocks.size }}">
        {% for block in section.blocks %}
          <div class="quotes-slide--{{ block.id }}" {{ block.shopify_attributes }}>
            <blockquote class="quotes-slider__text text-center">
              <span class="quote-icon">
                {% comment %}{% include 'icon-quote' %}{% endcomment %}
              	<img width="44" height="44" style="margin: auto;border-radius: 22px;object-fit: cover" {% if block.settings.author_img != blank %}src="{{ block.settings.author_img | img_url: 'master' }}"{% else %}
                     src="data:image/jpeg;base64," 
                     {% endif %} />
              </span>
                {% if block.settings.author_rev %}
                  <span class="quote-name">
                    <img width="100" style="margin: auto" src="{{ block.settings.author_rev | img_url : 'master' }}">
                  </span>
                {% endif %}
                {% if block.settings.quote_date != blank %}
                  <span class="date"><p>{{ block.settings.quote_date }}</p></span>
                {% endif %}
                {% if block.settings.quote_name != blank %}
                  <span class="quote-heading">
                    <h4>{{ block.settings.quote_name }}</h4>
                  </span>
                {% endif %}
              {% if block.settings.quote != blank %}
                <div class="rte-setting rte">{{ block.settings.quote }}</div>
              {% endif %}
              {% if block.settings.author != blank %}
                <cite>{{ block.settings.author | escape }}</cite>
              {% endif %}
              {% if block.settings.quote_days != blank %}
                  <span class="date"><p>{{ block.settings.quote_days }}</p></span>
              {% endif %}
            </blockquote>
          </div>
        {% endfor %}
      </div>
    </div>
  {% endif %}
  {% if section.settings.imgg %}
  <div class="img"style="text-align: center;">
  	<img width="150" height="75" style="margin: auto;object-fit: cover" src="{{ section.settings.imgg | img_url: 'master' }}" />
  </div>
  {% endif %}
  {% if section.blocks.size == 0 %}
    {% include 'no-blocks' %}
  {% endif %}
</div>



